<Window x:Class="Mrx.ApiClientGenerator.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mrx.ApiClientGenerator.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel LastChildFill="True">
        <StackPanel DockPanel.Dock="Top"  Orientation = "Horizontal">
            <Button x:Name = "button" Content = "New Profile" Margin = "10" Width = "120" Height = "30" />
            <Button x:Name = "button1" Content = "Duplicate" Margin = "10" Width = "120" Height = "30" />
            <Button x:Name = "button2" Content = "Save" Margin = "10" Width = "120" Height = "30" />
        </StackPanel>
        <TabControl x:Name="tabControl">
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>

    </DockPanel>
</Window>
